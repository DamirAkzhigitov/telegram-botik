{
  "name": "telegram-botik",
  "version": "0.0.0",
  "description": "A Telegram bot built with Telegraf and OpenAI API, deployed on Cloudflare Workers",
  "private": false,
  "type": "module",
  "license": "MIT",
  "keywords": [
    "telegram",
    "bot",
    "telegraf",
    "openai",
    "gpt-4",
    "cloudflare-workers",
    "wrangler",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DamirAkzhigitov/telegram-botik.git"
  },
  "author": "Damir Akzhigitov",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "dev:local": "tsx --env-file=.env.local ./src/local-test.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "validate": "pnpm format && pnpm typecheck && pnpm lint && pnpm test",
    "cf-typegen": "wrangler types",
    "serve": "ngrok http --url=better-suitably-monkey.ngrok-free.app 8787",
    "webhook:set-dev": "node scripts/setup-webhook.js set-dev",
    "webhook:set-prod": "node scripts/setup-webhook.js set-prod",
    "webhook:delete": "node scripts/setup-webhook.js delete",
    "webhook:info": "node scripts/setup-webhook.js info",
    "release": "semantic-release",
    "prepare": "husky"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.3",
    "@eslint/js": "^9.39.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.1",
    "@types/node": "22.18.13",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "2.1.9",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "semantic-release": "^25.0.1",
    "strip-literal": "^2.1.0",
    "tsx": "4.20.6",
    "typescript": "5.5.2",
    "typescript-eslint": "^8.46.2",
    "vitest": "2.1.9",
    "wrangler": "4.46.0"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "axios": "^1.13.1",
    "openai": "6.7.0",
    "telegraf": "4.16.3",
    "vite": "^6.4.1",
    "zod": "^4.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "workerd"
    ],
    "overrides": {
      "vite": "^6.4.1",
      "esbuild": "^0.25.11"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
