{
  "name": "my-first-worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "format": "prettier . --write",
    "dev:local": "tsx --env-file=.env.local ./src/local-test.ts",
    "start": "wrangler dev",
    "test": "vitest",
    "cf-typegen": "wrangler types",
    "serve": "ngrok http --url=better-suitably-monkey.ngrok-free.app 8787",
    "dev:full": "node scripts/start-dev.js",
    "deploy:prod": "node scripts/deploy-prod.js",
    "webhook:set-dev": "node scripts/setup-webhook.js set-dev",
    "webhook:set-prod": "node scripts/setup-webhook.js set-prod",
    "webhook:delete": "node scripts/setup-webhook.js delete",
    "webhook:info": "node scripts/setup-webhook.js info",
    "webhook:switch-dev": "node scripts/setup-webhook.js switch-dev",
    "webhook:switch-prod": "node scripts/setup-webhook.js switch-prod"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.8.60",
    "@types/node": "20.8.3",
    "prettier": "3.5.3",
    "tsx": "4.19.3",
    "typescript": "5.5.2",
    "vitest": "2.1.9",
    "wrangler": "4.28.0"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "axios": "^1.8.2",
    "openai": "5.15.0",
    "telegraf": "4.16.3",
    "vite": "^7.1.3",
    "zod": "^4.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "workerd"
    ]
  }
}
